<h3 class="header-facet">
    <%- displayName %>
</h3>
<ul data-facet="<%- name %>" class="facet-list collapse">
    <%= HtmlUtils.ensureHtml(optionsHtml) %>
</ul>
<% if (listIsHuge) { %>
    <div class="toggle ">
        <button class="show-more discovery-button">
            <%- gettext("More") %>
        </button>
        <button class="show-less hidden discovery-button">
            <%- gettext("Less") %>
        </button>
    </div>
<% } %>
